create table lwmon_data (
    topology varchar(255),
    host varchar(255),
    experiment varchar(255),
    schedule varchar(255),
    datasize int,
    run int,
    timestamp int,
    name varchar(255),
    parm varchar(255),
    key varchar(255),
    value int
);

create index lwmon_data_run
on lwmon_data(topology, experiment, schedule, datasize, run, host);

